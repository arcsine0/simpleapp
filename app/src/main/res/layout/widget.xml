<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/Widget.DialogBox.AppWidget.Container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00FFFFFF"
    android:theme="@style/Theme.DialogBox.AppWidgetContainer"
    app:circularflow_radiusInDP="10">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="413dp"
        android:layout_height="142dp"
        android:background="#191919"
        tools:layout_editor_absoluteX="-19dp"
        tools:layout_editor_absoluteY="-19dp"
        tools:ignore="MissingConstraints">

        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="43dp"
            android:layout_height="40dp"
            android:background="#00000000"
            app:srcCompat="@android:drawable/stat_sys_phone_call"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="327dp"
            tools:layout_editor_absoluteY="92dp" />

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="42dp"
            android:layout_height="40dp"
            android:background="#00FFFFFF"
            android:scaleX="1.75"
            android:scaleY="1.75"
            app:srcCompat="@android:drawable/stat_sys_speakerphone"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="93dp"
            tools:layout_editor_absoluteY="92dp" />

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="48dp"
            android:layout_height="38dp"
            android:background="#00FFFFFF"
            android:backgroundTint="#00FFFFFF"
            android:scaleX="1.25"
            android:scaleY="1.25"
            app:srcCompat="@android:drawable/ic_btn_speak_now"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="45dp"
            tools:layout_editor_absoluteY="92dp" />

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="34dp"
            android:layout_height="37dp"
            android:background="#00FFFFFF"
            android:scaleX="1.75"
            android:scaleY="1.75"
            app:srcCompat="@android:drawable/presence_video_busy"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="141dp"
            tools:layout_editor_absoluteY="92dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="288dp"
            android:layout_height="35dp"
            android:text="On Call: Diane Cabs"
            android:textAlignment="textStart"
            android:textColor="#F6F6F6"
            android:textSize="24sp"
            tools:layout_editor_absoluteX="82dp"
            tools:layout_editor_absoluteY="41dp"
            tools:ignore="MissingConstraints" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@android:drawable/ic_notification_overlay"
            tools:layout_editor_absoluteX="55dp"
            tools:layout_editor_absoluteY="51dp" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>